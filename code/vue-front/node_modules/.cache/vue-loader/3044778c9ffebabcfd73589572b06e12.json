em>\r\n        <el-breadcrumb-item class=\"item2\" v-for=\"(item, index) in breadcrumbItem\" :key=\"index\"><a>{{item.name}}</a></el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n    <div v-if=\"centerType\" :style='{\"border\":\"5px solid #FFC174\",\"width\":\"80%\",\"padding\":\"25px 30px\",\"margin\":\"10px auto\",\"borderRadius\":\"30px\",\"background\":\"linear-gradient(180deg, #10615F 0%, #0A4742 100%)\"}'>\r\n      <el-button size=\"mini\" @click=\"backClick\">返回</el-button>\r\n    </div>\r\n    <div class=\"list-preview\" :style='{\"padding\":\"0 10%\",\"margin\":\"10px auto\",\"alignItems\":\"flex-start\",\"flexWrap\":\"wrap\",\"background\":\"none\",\"display\":\"flex\",\"width\":\"100%\",\"position\":\"relative\"}'>\r\n      <el-form :inline=\"true\" :model=\"formSearch\" class=\"list-form-pv\" :style='{\"padding\":\"10px\",\"alignItems\":\"center\",\"flexWrap\":\"wrap\",\"background\":\"none\",\"display\":\"flex\",\"width\":\"100%\",\"height\":\"auto\",\"order\":\"1\"}'>\r\n        <el-form-item :style='{\"margin\":\"0 10px 10px\"}'>\r\n          <div class=\"lable\" v-if=\"true\" :style='{\"width\":\"auto\",\"padding\":\"0 10px\",\"lineHeight\":\"42px\",\"color\":\"#000\",\"display\":\"inline-block\"}'>活动名称：</div>\r\n          <el-input v-model=\"formSearch.activityName\" placeholder=\"活动名称\" @keydown.enter.native=\"getList(1, curFenlei)\" clearable></el-input>\r\n        </el-form-item>\r\n        <!-- 添加按钮前先判断是否已提交 -->\r\n        <el-button v-if=\"btnAuth('events','新增')\" :style='{\"cursor\":\"pointer\",\"border\":\"2px solid #FFC174\",\"padding\":\"0px 15px\",\"margin\":\"0 10px 10px 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"5px\",\"background\":\"#0D5451\",\"width\":\"auto\",\"fontSize\":\"14px\",\"lineHeight\":\"38px\",\"height\":\"42px\"}' type=\"primary\" @click=\"handleAdd('/index/eventsAdd')\">\r\n          <i v-if=\"true\" :style='{\"color\":\"#fff\",\"margin\":\"0 10px 0 0\",\"fontSize\":\"14px\"}' class=\"el-icon-circle-plus-outline\"></i>添加\r\n        </el-button>\r\n      </el-form>\r\n      <div class=\"list\" :style='{\"width\":\"calc(100% - 400px)\",\"margin\":\"0 0 10px\",\"background\":\"#fff\",\"flex\":\"1\",\"order\":\"3\"}'>\r\n        <!-- 样式一 -->\r\n        <div class=\"list1 index-pv1\" :style='{\"border\":\"5px solid #FFC174\",\"padding\":\"10px 0\",\"borderRadius\":\"10px\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"100%\",\"height\":\"auto\"}'>\r\n          <div :style='{\"border\":\"5px solid #FFC174\",\"padding\":\"10px\",\"margin\":\"0 15px 30px\",\"borderRadius\":\"30px\",\"color\":\"#000\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"display\":\"flex\",\"width\":\"calc(25% - 30px)\",\"position\":\"relative\",\"height\":\"auto\"}' v-for=\"(item, index) in dataList\" :key=\"index\" @click.stop=\"toDetail(item)\" class=\"list-item animation-box\">\r\n            <img @click.stop=\"imgPreView(item.coverImage)\" :style='{\"width\":\"100%\",\"objectFit\":\"cover\",\"borderRadius\":\"30px\",\"display\":\"block\",\"height\":\"275px\",\"order\":\"1\"}' v-if=\"item.coverImage && item.coverImage.substr(0,4)=='http'\" :src=\"item.coverImage.split(',')[0]\" class=\"image\" />\r\n            <img @click.stop=\"imgPreView(baseUrl + (item.coverImage?item.coverImage.split(',')[0]:'') )\" :style='{\"width\":\"100%\",\"objectFit\":\"cover\",\"borderRadius\":\"30px\",\"display\":\"block\",\"height\":\"275px\",\"order\":\"1\"}' v-else :src=\"baseUrl + (item.coverImage?item.coverImage.split(',')[0]:'')\" class=\"image\" />\r\n            <div v-if=\"item.price\" :style='{\"padding\":\"0 10px\",\"margin\":\"-50px 0 0\",\"color\":\"red\",\"borderRadius\":\"30px\",\"textAlign\":\"center\",\"background\":\"#FFC174\",\"width\":\"100%\",\"lineHeight\":\"60px\",\"fontSize\":\"24px\",\"order\":\"2\",\"zIndex\":\"1\"}' class=\"price\">\r\n              <span :style='{\"fontSize\":\"12px\"}'>￥</span>{{item.price}}\r\n            </div>\r\n            <div :style='{\"padding\":\"0 10px\",\"color\":\"inherit\",\"textAlign\":\"center\",\"width\":\"100%\",\"lineHeight\":\"2\",\"fontSize\":\"16px\",\"fontWeight\":\"bold\",\"order\":\"3\"}' class=\"name \">{{item.activityName}}</div>\r\n            <div :style='{\"padding\":\"0 5px\",\"order\":\"8\"}'>\r\n              <span class=\"icon iconfont icon-shijian21\" :style='{\"width\":\"100%\",\"margin\":\"0 2px 0 0\",\"lineHeight\":\"1.5\",\"fontSize\":\"14px\",\"color\":\"inherit\"}'></span>\r\n              <span class=\"text\" :style='{\"color\":\"inherit\",\"lineHeight\":\"1.5\",\"fontSize\":\"14px\"}'>{{item.addTime}}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <el-pagination\r\n          background\r\n          id=\"pagination\"\r\n          class=\"pagination\"\r\n          :pager-count=\"7\"\r\n          :page-size=\"pageSize\"\r\n          prev-text=\"上一页\"\r\n          next-text=\"下一页\"\r\n          :hide-on-single-page=\"true\"\r\n          :layout='[\"prev\",\"pager\",\"next\"].join()'\r\n          :total=\"total\"\r\n          :style='{\"width\":\"1200px\",\"padding\":\"0\",\"margin\":\"10px auto\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"fontWeight\":\"500\"}'\r\n          @current-change=\"curChange\"\r\n          @size-change=\"sizeChange\"\r\n          @prev-click=\"prevClick\"\r\n          @next-click=\"nextClick\"\r\n      ></el-pagination>\r\n    </div>\r\n    <el-dialog title=\"预览图\" :visible.sync=\"previewVisible\" width=\"50%\">\r\n      <img :src=\"previewImg\" alt=\"\" style=\"width: 100%;\">\r\n    </el-dialog>\r\n    <!-- 新增：报名提示弹窗 -->\r\n    <el-dialog title=\"报名提示\" :visible.sync=\"registerMsgVisible\" width=\"30%\">\r\n      <div style=\"text-align:center; padding:20px 0;\">\r\n        已报名活动，请等待管理员审核，切勿重复提交\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"registerMsgVisible = false\">确定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      layouts: '',\r\n      swiperIndex: -1,\r\n      baseUrl: '',\r\n      breadcrumbItem: [\r\n        {\r\n          name: '摄影活动'\r\n        }\r\n      ],\r\n      formSearch: {\r\n        activityName: ''\r\n      },\r\n      fenlei: [],\r\n      feileiColumn: '',\r\n      dataList: [],\r\n      total: 1,\r\n      pageSize: 12,\r\n      totalPage: 1,\r\n      curFenlei: '全部',\r\n      isPlain: false,\r\n      indexQueryCondition: '',\r\n      timeRange: [],\r\n      centerType: false,\r\n      previewImg: '',\r\n      previewVisible: false,\r\n      sortType: 'id',\r\n      sortOrder: 'desc',\r\n      // 新增：报名状态标识（示例中为 true 表示已报名）\r\n      hasRegistered: true,\r\n      // 控制报名提示弹窗显示\r\n      registerMsgVisible: false\r\n    }\r\n  },\r\n  async created() {\r\n    if(this.$route.query.centerType){\r\n      this.centerType = true\r\n    }\r\n    this.baseUrl = this.$config.baseUrl;\r\n    this.getFenlei();\r\n    this.getList(1, '全部');\r\n  },\r\n  methods: {\r\n    // 新增：触发报名提示弹窗\r\n    triggerRegisterMsg() {\r\n      this.registerMsgVisible = true;\r\n    },\r\n    // 修改 add 方法：判断是否已报名\r\n    handleAdd(path) {\r\n      if(this.hasRegistered) {\r\n        // 如果已报名则触发弹窗提示\r\n        this.triggerRegisterMsg();\r\n      } else {\r\n        let query = {};\r\n        if(this.centerType){\r\n          query.centerType = 1;\r\n        }\r\n        this.$router.push({ path: path, query: query });\r\n      }\r\n    },\r\n    getFenlei() {\r\n      // 实现获取分类信息的逻辑\r\n    },\r\n    getList(page, fenlei, ref = '') {\r\n      let params = { page, limit: this.pageSize };\r\n      let searchWhere = {};\r\n      if (this.formSearch.activityName !== '') {\r\n        searchWhere.activityName = '%' + this.formSearch.activityName + '%';\r\n      }\r\n      if (this.sortType) searchWhere.sort = this.sortType;\r\n      if (this.sortOrder) searchWhere.order = this.sortOrder;\r\n      this.$http.get(`events/${this.centerType ? 'page' : 'list'}`, { params: Object.assign(params, searchWhere) }).then(res => {\r\n        if (res.data.code === 0) {\r\n          this.dataList = res.data.data.list;\r\n          this.total = Number(res.data.data.total);\r\n          this.pageSize = Number(res.data.data.pageSize);\r\n          this.totalPage = res.data.data.totalPage;\r\n        }\r\n      });\r\n    },\r\n    curChange(page) {\r\n      this.getList(page);\r\n    },\r\n    prevClick(page) {\r\n      this.getList(page);\r\n    },\r\n    sizeChange(size) {\r\n      this.pageSize = size;\r\n      this.getList(1);\r\n    },\r\n    nextClick(page) {\r\n      this.getList(page);\r\n    },\r\n    imgPreView(url) {\r\n      this.previewImg = url;\r\n      this.previewVisible = true;\r\n    },\r\n    toDetail(item) {\r\n      let params = {\r\n        id: item.id\r\n      };\r\n      if (this.centerType) {\r\n        params.centerType = 1;\r\n      }\r\n      this.$router.push({ path: '/index/eventsDetail', query: params });\r\n    },\r\n    btnAuth(tableName, key) {\r\n      if (this.centerType) {\r\n        return this.isBackAuth(tableName, key);\r\n      } else {\r\n        return this.isAuth(tableName, key);\r\n      }\r\n    },\r\n    backClick() {\r\n      this.$router.push({ path: '/index/center' });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style rel=\"stylesheet/scss\" lang=\"scss\" scoped>\r\n/* 原有样式代码保持不变 */\r\n.list-preview .list-form-pv .el-input {\r\n  width: auto;\r\n}\r\n\r\n.breadcrumb-preview .el-breadcrumb /deep/ .el-breadcrumb__separator {\r\n  margin: 0 15px;\r\n  color: #FFC174;\r\n  font-weight: 500;\r\n}\r\n\r\n.breadcrumb-preview .el-breadcrumb .item1 /deep/ .el-breadcrumb__inner a {\r\n  color: #FFC174;\r\n  display: inline-block;\r\n}\r\n\r\n.breadcrumb-preview .el-breadcrumb .item2 /deep/ .el-breadcrumb__inner a {\r\n  color: #FFC174;\r\n  display: inline-block;\r\n}\r\n\r\n.category-1 .item {\r\n  cursor: poin