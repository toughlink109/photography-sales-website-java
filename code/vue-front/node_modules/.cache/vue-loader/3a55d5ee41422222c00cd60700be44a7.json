 placeholder=\"请输入账号\"\r\n          />\r\n        </el-form-item>\r\n\r\n        <el-form-item\r\n            v-if=\"loginType==1\"\r\n            class=\"list-item\"\r\n            :style='{\r\n            width:\"100%\",\r\n            margin:\"0 0 10px\",\r\n            position:\"relative\",\r\n            order:\"2\"\r\n          }'\r\n            prop=\"password\"\r\n        >\r\n          <div\r\n              v-if=\"true\"\r\n              :style='{\r\n              color: \"#fff\",\r\n              left: \"-120px\",\r\n              textAlign: \"right\",\r\n              width: \"100px\",\r\n              lineHeight: \"44px\",\r\n              fontSize: \"14px\",\r\n              position: \"absolute\"\r\n            }'\r\n          >\r\n            密码：\r\n          </div>\r\n          <input\r\n              :style='{\r\n              border: \"2px solid #909090\",\r\n              padding: \"0 10px\",\r\n              boxShadow: \"none\",\r\n              outline: \"none\",\r\n              color: \"#fff\",\r\n              outlineOffset: \"0\",\r\n              borderRadius: \"5px\",\r\n              background: \"#D8D8D850\",\r\n              width: \"500px\",\r\n              fontSize: \"15px\",\r\n              height: \"60px\"\r\n            }'\r\n              v-model=\"loginForm.password\"\r\n              placeholder=\"请输入密码\"\r\n              type=\"password\"\r\n          />\r\n        </el-form-item>\r\n\r\n        <el-form-item\r\n            class=\"list-type select\"\r\n            :style='{\r\n            width:\"100%\",\r\n            margin:\"0 0 10px\",\r\n            position:\"relative\",\r\n            order:\"2\"\r\n          }'\r\n            v-if=\"roles.length>1\"\r\n        >\r\n          <el-select\r\n              v-model=\"loginForm.tableName\"\r\n              placeholder=\"请选择角色\"\r\n              @change=\"selectChange\"\r\n          >\r\n            <el-option\r\n                v-for=\"(item,index) in roles\"\r\n         