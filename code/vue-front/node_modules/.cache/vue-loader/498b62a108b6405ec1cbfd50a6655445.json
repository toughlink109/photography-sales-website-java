\":\"no-repeat\",\"justifyContent\":\"center\"}'>\r\n      <el-form class=\"rgs-form\" v-if=\"pageFlag=='register'\"\r\n               :style='{\"padding\":\"200px 20px 50px\",\"boxShadow\":\"none\",\"margin\":\"20px 0 40px\",\"alignItems\":\"center\",\"flexDirection\":\"column\",\"display\":\"flex\",\"borderRadius\":\"10px\",\"flexWrap\":\"wrap\",\"background\":\"url(http://codegen.caihongy.cn/20240109/cb7f19f1621648a1aa900738964dddd9.png)\",\"width\":\"45vw\",\"backgroundSize\":\"100% 100%\",\"backgroundPosition\":\"center center\",\"backgroundRepeat\":\"no-repeat\",\"height\":\"auto\"}'\r\n               ref=\"registerForm\" :model=\"registerForm\" :rules=\"rules\">\r\n        <div :style='{\"margin\":\"0 0 10px 0\",\"color\":\"#fff\",\"textAlign\":\"center\",\"width\":\"100%\",\"lineHeight\":\"44px\",\"fontSize\":\"24px\"}'>\r\n          基于Java的个人摄影作品管理网站注册\r\n        </div>\r\n\r\n        <!-- 账号：必填项 -->\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"account\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"rgba(64, 158, 255, 1)\"}'>\r\n            <span class=\"required\">*</span>账号名称：\r\n          </div>\r\n          <el-input v-model=\"registerForm.account\" placeholder=\"请输入账号\"/>\r\n        </el-form-item>\r\n\r\n        <!-- 密码：必填项 -->\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"password\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"rgba(64, 158, 255, 1)\"}'>\r\n            <span class=\"required\">*</span>密码：\r\n          </div>\r\n          <el-input v-model=\"registerForm.password\" type=\"password\" placeholder=\"请输入密码\"/>\r\n        </el-form-item>\r\n\r\n        <!-- 确认密码：必填项 -->\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"password2\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"rgba(64, 158, 255, 1)\"}'>\r\n            <span class=\"required\">*</span>确认密码：\r\n          </div>\r\n          <el-input v-model=\"registerForm.password2\" type=\"password\" placeholder=\"请再次输入密码\"/>\r\n        </el-form-item>\r\n\r\n        <!-- 非必填项展示 -->\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"realName\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            姓名：\r\n          </div>\r\n          <el-input v-model=\"registerForm.realName\" placeholder=\"请输入姓名\"/>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"userSex\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            性别：\r\n          </div>\r\n          <el-select v-model=\"registerForm.userSex\" placeholder=\"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in ordinaryUseruserSexOptions\"\r\n                :key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"mobile\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            手机：\r\n          </div>\r\n          <el-input v-model=\"registerForm.mobile\" placeholder=\"请输入手机\"/>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"email\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            邮箱：\r\n          </div>\r\n          <el-input v-model=\"registerForm.email\" placeholder=\"请输入邮箱\"/>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"idCard\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            身份证：\r\n          </div>\r\n          <el-input v-model=\"registerForm.idCard\" placeholder=\"请输入身份证\"/>\r\n        </el-form-item>\r\n        <el-form-item :style='{\"width\":\"auto\",\"padding\":\"0\",\"margin\":\"0 auto 15px\",\"height\":\"auto\"}'\r\n                      v-if=\"tableName=='ordinaryUser'\" prop=\"userAvatar\">\r\n          <div :style='{\"width\":\"auto\",\"lineHeight\":\"44px\",\"fontSize\":\"14px\",\"color\":\"#fff\"}'>\r\n            头像：\r\n          </div>\r\n          <file-upload\r\n              tip=\"点击上传头像\"\r\n              action=\"file/upload\"\r\n              :limit=\"1\"\r\n              :multiple=\"true\"\r\n              :fileUrls=\"registerForm.userAvatar ? registerForm.userAvatar : ''\"\r\n              @change=\"ordinaryUseruserAvatarUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n        <el-button\r\n            :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 10px\",\"boxShadow\":\"none\",\"margin\":\"20px auto\",\"color\":\"#fff\",\"display\":\"block\",\"outline\":\"none\",\"borderRadius\":\"8px\",\"background\":\"#FFC174\",\"width\":\"40%\",\"fontSize\":\"18px\",\"fontWeight\":\"bold\",\"height\":\"44px\"}'\r\n            type=\"primary\" @click=\"submitForm('registerForm')\">注册\r\n        </el-button>\r\n        <el-button\r\n            :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 10px\",\"boxShadow\":\"0 0 6px rgba(64, 158, 255, .5)\",\"margin\":\"20px auto 5px\",\"color\":\"#fff\",\"display\":\"none\",\"outline\":\"none\",\"borderRadius\":\"8px\",\"background\":\"rgba(64, 158, 255, 1)\",\"width\":\"80%\",\"fontSize\":\"16px\",\"height\":\"44px\"}'\r\n            @click=\"resetForm('registerForm')\">重置\r\n        </el-button>\r\n        <router-link\r\n            :style='{\"cursor\":\"pointer\",\"padding\":\"0 10%\",\"color\":\"#fff\",\"display\":\"inline-block\",\"lineHeight\":\"1\",\"fontSize\":\"14px\",\"textDecoration\":\"none\"}'\r\n            to=\"/login\">已有账户登录\r\n        </router-link>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      pageFlag: '',\r\n      tableName: '',\r\n      registerForm: {},\r\n      rules: {},\r\n      // 用于标识必填项（只针对账号、密码、确认密码）\r\n      requiredRules: {},\r\n      ordinaryUseruserSexOptions: [],\r\n    }\r\n  },\r\n  mounted() {\r\n    if (this.$route.query.pageFlag === 'register') {\r\n      this.tableName = this.$route.query.role;\r\n      if (this.tableName === 'ordinaryUser') {\r\n        this.registerForm = {\r\n          account: '',\r\n          password: '',\r\n          password2: '',\r\n          realName: '',\r\n          userSex: '',\r\n          mobile: '',\r\n          email: '',\r\n          idCard: '',\r\n          userAvatar: '',\r\n          money: '',\r\n          vip: '',\r\n        }\r\n        // 为必填项添加标识\r\n        this.requiredRules.account = [{ required: true, message: '请输入账号', trigger: 'blur' }];\r\n        this.requiredRules.password = [{ required: true, message: '请输入密码', trigger: 'blur' }];\r\n        this.requiredRules.password2 = [{ required: true, message: '请再次输入密码', trigger: 'blur' }];\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.pageFlag = this.$route.query.pageFlag;\r\n    if (this.$route.query.pageFlag === 'register') {\r\n      if (this.tableName === 'ordinaryUser') {\r\n        // 账号\r\n        this.rules.account = [{ required: true, message: '请输入账号', trigger: 'blur' }];\r\n        // 密码：校验规则：至少6位，且包含大小写字母和特殊符号\r\n        this.rules.password = [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n          { validator: this.validatePassword, trigger: 'blur' }\r\n        ];\r\n        // 确认密码：不仅必填还要验证两次输入是否一致\r\n        this.rules.password2 = [\r\n          { required: true, message: '请再次输入密码', trigger: 'blur' },\r\n          { validator: this.validatePassword2, trigger: 'blur' }\r\n        ];\r\n        // 手机号校验（示例，可根据需要自行调整）\r\n        this.rules.mobile = [{ required: false, validator: this.$validate.isMobile, trigger: 'blur' }];\r\n        // 金额校验（示例）\r\n        this.rules.money = [{ required: false, validator: this.$validate.isNumber, trigger: 'blur' }];\r\n      }\r\n      // 性别选项\r\n      this.ordinaryUseruserSexOptions = \"男,女\".split(',');\r\n    }\r\n  },\r\n  methods: {\r\n    // 判断是否为必填项（根据requiredRules判断）\r\n    changeRules(name) {\r\n      return !!this.requiredRules[name]\r\n    },\r\n    // 密码校验函数：至少6位且包含大小写字母和特殊符号\r\n    validatePassword(rule, value, callback) {\r\n      const pattern = /^(?=.*[a-z])(?=.*[A-Z])(?=.*[\\W_]).{6,}$/;\r\n      if (!value) {\r\n        return callback(new Error('请输入密码'));\r\n      }\r\n      if (!pattern.test(value)) {\r\n        return callback(new Error('密码至少6位，需包含大小写字母和特殊符号'));\r\n      }\r\n      callback();\r\n    },\r\n    // 确认密码校验：判断两次输入是否一致\r\n    validatePassword2(rule, value, callback) {\r\n      if (!value) {\r\n        return callback(new Error('请再次输入密码'));\r\n      }\r\n      if (value !== this.registerForm.password) {\r\n        return callback(new Error('两次密码输入不一致'));\r\n      }\r\n      callback();\r\n    },\r\n    // 获取uuid\r\n    getUUID() {\r\n      return new Date().getTime();\r\n    },\r\n    ordinaryUseruserAvatarUploadChange(fileUrls) {\r\n      this.registerForm.userAvatar = fileUrls.replace(new RegExp(this.$config.baseUrl, \"g\"), \"\");\r\n    },\r\n    submitForm(formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          const url = this.tableName + \"/register\";\r\n          if (this.tableName === 'ordinaryUser') {\r\n            if (!this.registerForm.account) {\r\n              this.$message.error('账号不能为空');\r\n              return;\r\n            }\r\n            if (this.registerForm.password !== this.registerForm.password2) {\r\n              this.$message.error('两次密码输入不一致');\r\n              return;\r\n            }\r\n            if (!this.registerForm.password) {\r\n              this.$message.error('密码不能为空');\r\n              return;\r\n            }\r\n            if (this.registerForm.mobile && !this.$validate.isMobile2(this.registerForm.mobile)) {\r\n              this.$message.error('手机应输入手机格式');\r\n              return;\r\n            }\r\n          }\r\n          this.$http.post(url, this.registerForm).then(res => {\r\n            if (res.data.code === 0) {\r\n              this.$message({\r\n                message: '注册成功',\r\n                type: 'success',\r\n                duration: 1500,\r\n                onClose: () => {\r\n                  this.$router.push('/login');\r\n                }\r\n              });\r\n            } else {\r\n              this.$message.error(res.data.msg);\r\n            }\r\n          });\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style rel=\"stylesheet/scss\" lang=\"scss\" scoped>\r\n.container {\r\n  position: relative;\r\n  background: url(http://codegen.caihongy.cn/20240109/1e2e4f261a334615ab068d77c10daf3f.png);\r\n\r\n  .el-input {\r\n    width: auto;\r\n  }\r\n\r\n  .el-date-editor.el-input {\r\n    width: auto;\r\n  }\r\n\r\n  .el-form-item /deep/ .el-form-item__content {\r\n    display: flex;\r\n  }\r\n\r\n  .rgs-form .el-input /deep/ .el-input__inner {\r\n    border: 2px solid #808080;\r\n    border-radius: 5px;\r\n    padding: 0 10px;\r\n    box-shadow: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background: #D8D8D850;\r\n    width: 25vw;\r\n    font-size: 14px;\r\n    height: 40px;\r\n  }\r\n\r\n  .rgs-form .el-select /deep/ .el-input__inner {\r\n    border: 2px solid #808080;\r\n    border-radius: 5px;\r\n    padding: 0 10px;\r\n    box-shadow: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background: #D8D8D850;\r\n    width: 25vw;\r\n    font-size: 14px;\r\n    height: 40px;\r\n  }\r\n\r\n  .rgs-form .el-date-editor /deep/ .el-input__inner {\r\n    border: 2px solid #808080;\r\n    border-radius: 5px;\r\n    padding: 0 10px 0 30px;\r\n    box-shadow: none;\r\n    outline: none;\r\n    color: #fff;\r\n    background: #D8D8D850;\r\n    width: 25vw;\r\n    font-size: 14px;\r\n    height: 40px;\r\n  }\r\n\r\n  .rgs-form /deep/ .el-upload--picture-card {\r\n    background: transparent;\r\n    border: 0;\r\n    border-radius: 0;\r\n    width: auto;\r\n    height: auto;\r\n    line-height: initial;\r\n    vertical-align: middle;\r\n  }\r\n\r\n  .rgs-form /deep/ .upload .upload-img,\r\n  .rgs-form /deep/ .el-upload-list .el-upload-list__item,\r\n  .rgs-form /deep/ .el-upload .el-icon-plus {\r\n    border: 2px solid #808080;\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n    color: #999;\r\n    background: #D8D8D850;\r\n    width: 100px;\r\n    font-size: 32px;\r\n    line-height: 100px;\r\n    text-align: center;\r\n    height: 100px;\r\n  }\r\n}\r\n\r\n.required {\r\n  color: red;\r\n}\r\n</style>\r\n"]}]}